<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="603.0" prefWidth="788.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.SecondaryController">
  <children>
    <AnchorPane prefHeight="568.0" prefWidth="748.0">
      <children>
        <Button fx:id="SOSbutton" layoutX="64.0" layoutY="482.0" mnemonicParsing="false" onAction="#handlePressingSOS" prefHeight="27.0" prefWidth="62.0" style="-fx-background-color: F7EFE7; -fx-border-color: D30F0F;" text="SOS" />
        <Button fx:id="secondaryButton" layoutX="276.0" layoutY="483.0" onAction="#switchToPrimary" text="Switch to Primary View" />
        <TableView fx:id="userTable" layoutX="127.0" layoutY="47.0" prefHeight="380.0" prefWidth="436.0">
          <columns>
            <TableColumn fx:id="user_id" prefWidth="75.0" text="id" />
            <TableColumn fx:id="user_userName" prefWidth="75.0" text="userName" />
            <TableColumn fx:id="user_gender" prefWidth="75.0" text="gender" />
            <TableColumn fx:id="user_password" prefWidth="75.0" text="password" />
            <TableColumn fx:id="user_age" prefWidth="75.0" text="age" />
            <TableColumn fx:id="user_community" prefWidth="75.0" text="community" />
          </columns>
        </TableView>
      </children>
    </AnchorPane>
  </children>
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="40.0" />
  </padding>
</VBox>
